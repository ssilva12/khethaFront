div(class="modal", id="modalWeight", role="dialog")
    div(role="document", class="modal-dialog")
        div(class="modal-content")
            div(class="modal-header")
                button(type="button", data-dismiss="modal", class="close")
                    span(aria-hidden="true") &times;
                    span(class="sr-only") Close
                h4(class="modal-title", id="weightLabel") Editar peso
            div(class="modal-body")
                div() 
                    label() Metha feature:&nbsp;
                    span() {{ Dato.featureEdit.methaFeature.name }},&nbsp; 
                    label() Feature:&nbsp;
                    span() {{ Dato.featureEdit.name }},&nbsp;
                    label() MR:&nbsp;
                    span() {{ Dato.methaRelationEdit }}
                div(class="form-group")
                    label(for='weight') Peso:
                    input(id="weight", class="form-control", type='text', placeholder='peso', name='weight', ng-model="Dato.weightPopupEdit")
            div(class="modal-footer")
                button(class="btn btn-default", type="button", ng-click="saveWeight()", data-dismiss="modal") Guardar
                button(class="btn btn-default", type="button", data-dismiss="modal") Cancelar
div(class="modal", id="modalAddFeature", role="dialog")
    div(class="modal-dialog", role="document") 
        div(class="modal-content")
            div(class="modal-header")
                button(type="button", data-dismiss="modal", class="close")
                    span(aria-hidden="true") &times;
                    span(class="sr-only") Close
                h4(class="modal-title", id="weightLabel") Agregar característica
            div(class="modal-body")
                div() 
                    label() Metha feature:&nbsp;
                    span() {{ Dato.currentMethaFeatureName }}
                div(class="form-group")
                    label(for='feature') Caracteristica:
                    select(ng-model="Dato.featureToAdd")
                        option(ng-repeat="f in Dato.currentFeatureNames")(value="{{ f.id }}") {{ f.name }}
                div(class="alert alert-danger", ng-show="Dato.addFeatureError") {{ Dato.addFeatureError }}
            div(class="modal-footer")
                button(class="btn btn-default", type="button", ng-click="addFeature()") Guardar
                button(class="btn btn-default", type="button", data-dismiss="modal") Cancelar
div(class="modal", id="modalDiscarded", role="dialog")
    div(class="modal-dialog", role="document")
        div(class="modal-content")
            div(class="modal-header")
                button(type="button", data-dismiss="modal", class="close")
                    span(aria-hidden="true") &times;
                    span(class="sr-only") Close
                h4(class="modal-title") Modificar discarded
            div(class="modal-body")
                div() 
                    label() Metha feature:&nbsp; 
                    span() {{ Dato.featureEdit.methaFeature.name }}, 
                    label() Feature:&nbsp;
                    span() {{ Dato.featureEdit.name }}
                div() Cambiar discarded a: {{ !Dato.featureEdit.discarded }}
                div(class="alert alert-danger",ng-show="Dato.editDiscardedError") {{ Dato.editDiscardedError }}
            div(class="modal-footer")
                button(class="btn btn-default", type="button", ng-click="saveDiscarded()") Confirmar
                button(class="btn btn-default", type="button", data-dismiss="modal") Cancelar
