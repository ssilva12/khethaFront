                                                        a(data-toggle="tab", data-target="#tab2") Peso
                                                    li(ng-class="{'active' : Dato.tab == 'tab3'}",ng-show="Dato.vacancy != ''")
                                                        a(data-toggle="tab", data-target="#tab3") Peso establecido
                                                    li(ng-class="{'active' : Dato.tab == 'tab4'}")
