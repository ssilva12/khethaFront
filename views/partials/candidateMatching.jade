//- PAGE TITLE
div(class="page-title")
    h2() Caracterización de vacantes
//- END PAGE TITLE
//- PAGE CONTENT WRAPPER
div(class="page-content-wrap")
    div(class="row")
        div(class="col-md-12")
            //-INICIO
            div(class="row")
                div(class="col-md-12")
                    div(class="panel panel-default")
                        div(class="panel-body")
                            form(class="form-horizontal")
                                div(class="form-group")
                                    div(class="row row-space")
                                        div(class="col-sm-1")
                                            label() Empleador:
                                        div(class="col-sm-3")
                                            select(class="form-control", ng-model="Dato.employer", ng-change="getJobs()")
                                                option(value="") Todos
                                                option(ng-repeat="emp in Dato.employers")(value="{{ emp.id }}") {{ emp.name }}
                                        div(class="col-sm-1")
                                            label() Oficio:
                                        div(class="col-sm-3")
                                            select(class="form-control", ng-model="Dato.job", ng-change="getVacancies()")
                                                option(value="") Todos
                                                option(ng-repeat="job in Dato.jobs")(value="{{ job.id }}") {{ job.name }}
                                        div(class="col-sm-1")
                                            label() Vacante:
                                        div(class="col-sm-3")
                                            select(class="form-control", ng-model="Dato.vacancy")
                                                option(value="") Todos
                                                option(ng-repeat="vac in Dato.vacancies")(value="{{ vac.id }}") {{ vac.name }}
                                    div(class="row row-space")
                                        div(class="col-sm-2")
                                            label() Año de referencia:
                                        div(class="col-sm-2")
                                            select(class="form-control", ng-model="Dato.referenceYear")
                                                option(value="")
                                                option(ng-repeat="y in Dato.possibleReferenceYears", value="{{ y }}") {{ y }} 
                                        div(class="col-sm-2")
                                            label() Años atras:
                                        div(class="col-sm-2")
                                            select(class="form-control", ng-model="Dato.yearsBack")
                                                option(value="")
                                                option(ng-repeat="y in Dato.possibleYearsBack", value="{{ y }}") {{ y }} 
                                        div(class="col-sm-3")
                                            label() Porcentaje ocurrencias(%):
                                        div(class="col-sm-1")
                                            input(class="form-control", type="text", ng-model="Dato.minPercentage")
                                    div(class="row row-space")
                                        div(class="col-sm-6") Total candidatos: {{ methaFeatures[0].totalCount }}
                                        div(class="col-sm-6")
                                            div(class="btn-group pull-right")
                                                button(type="button", class="btn btn-default", ng-click="getMethaFeaturesJobLastVacancy()")
                                                    li(class="fa fa-search")
                                                    span() Consultar Job Last Vacancy
                                                button(type="button", class="btn btn-default", ng-click="calculateFrequencyMatrix()") 
                                                    li(class="fa fa-search")
                                                    span() Calcular
                                                button(type="button", class="btn btn-default", ng-click="getMethaFeatures()") 
                                                    li(class="fa fa-search")
                                                    span() Consultar
                                    div(class="row")
                                        div(class="col-sm-12")
