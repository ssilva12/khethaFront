                                    li(class="dd-item", data-id="1", ng-repeat="exp in usuario.experiencia")
                                        div(class="dd-handle clearfix")
                                            div(class="form-group")
                                                div(class="col-md-9")
                                                    div(class="form-group")
                                                        div(class="col-md-3")
