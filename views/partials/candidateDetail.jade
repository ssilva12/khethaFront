                                                            p(ng-bind="estudio.estudio", ng-hide="estudio.edit")
                                                            div(style="width:100%", ng-show="estudio.edit")
                                                                select(style="width:100%",class="form-control", select2="", name="estudio", data-ng-model="estudio.estudio", ng-disabled="!estudio.edit")
                                                                    option(value="") Seleccione
                                                                    option(ng-repeat="estudio in variablesGlobales.estudios", value="estudio.value") {{estudio.label}}
                                                    div(class="form-group")
                                                        div(class="col-md-3")
                                                            label(for="") Grado
                                                        div(class="col-md-9")
                                                            p(ng-bind="estudio.grado", ng-hide="estudio.edit")
                                                            div(style="width:100%", ng-show="estudio.edit")
                                                                select(style="width:100%",class="form-control", select2="", name="estudio", data-ng-model="estudio.grado", ng-options="grado.value as grado.label for grado in variablesGlobales.grados", ng-disabled="!estudio.edit")
                                                                    option(value="") Seleccione
                                                    div(class="form-group")    
                                                        div(class="col-md-3")
                                                            label(for="") Fecha de terminación
                                                        div(class="col-md-9")
                                                            p(ng-bind="estudio.fechaTerminacion", ng-hide="estudio.edit")
                                                            input(ng-model="estudio.fechaTerminacion", type="text", class="form-control", ng-show="estudio.edit")
                                                    div(class="form-group")
                                                        div(class="col-md-3")
                                                            label(for="") Pais
                                                        div(class="col-md-9")
                                                            p(ng-bind="estudio.pais", ng-hide="estudio.edit")
                                                            div(style="width:100%", ng-show="estudio.edit")
                                                                select(style="width:100%",class="form-control", select2="", name="pais", data-ng-model="estudio.pais", ng-options="pais.value as pais.label for pais in variablesGlobales.paises", ng-disabled="!estudio.edit")
                                                                    option(value="") Seleccione
                                                div(class="col-md-3")
                                                    ul(class="panel-controls")
                                                        li()
                                                            a(class="panel-fullscreen", ng-click="estudio.edit = !estudio.edit")
                                                                span(class="fa fa-pencil", ng-hide="estudio.edit")
                                                                span(class="fa fa-save", ng-show="estudio.edit")
                                                        li()
                                                            a(class="panel-remove", ng-click="eliminarEstudio(estudio)")
                                                                span(class="fa fa-times")
                    div(class="form-group")
                        div(class="col-md-12")
                            button(type="button", class="btn btn-primary pull-right", ng-click="agregarEstudio()") Agregar
        div(class="col-md-6")
            div(class="panel panel-default")
                div(class="panel-heading")
                    h3(class="panel-title") Centros educativos
                 div(class="panel-body")
                    div(class="form-group")
                        div(class="col-md-12")
                            button(type="button", class="btn btn-primary pull-right", ng-click="agregarCentroEstudio()") Agregar
                    div(class="form-group")
                        div(class="col-md-12")
                            div(class="dd", id="nestable")
                                ol(class="dd-list")
                                    li(class="dd-item", data-id="1", ng-repeat="estudio in usuario.schooling")
                                        div(class="dd-handle clearfix")
                                            div(class="form-group")
                                                div(class="col-md-9")
                                                    div(class="form-group")
                                                        div(class="col-md-3")
                                                            label(for="") Nombre
                                                        div(class="col-md-9")
                                                            p(ng-bind="estudio.jobName", ng-hide="estudio.edit")
                                                            div(style="width:100%", ng-show="estudio.edit")
                                                                select(style="width:100%",class="form-control", select2="", name="centroEducativo", data-ng-model="estudio.jobName", ng-disabled="!estudio.edit")
                                                                    option(value="") Seleccione
                                                                    option(ng-repeat="centro in variablesGlobales.centrosEducativos", value="centro.value") {{centro.label}}
                                                    div(class="form-group")
                                                        div(class="col-md-3")
                                                            label(for="") Pais
                                                        div(class="col-md-9")
                                                            p(ng-bind="estudio.pais", ng-hide="estudio.edit")
                                                            div(style="width:100%", ng-show="estudio.edit")
                                                                select(style="width:100%",class="form-control", select2="", name="pais", data-ng-model="estudio.pais", ng-options="pais.value as pais.label for pais in variablesGlobales.paises", ng-disabled="!estudio.edit")
                                                                    option(value="") Seleccione
                                                div(class="col-md-3")
                                                    ul(class="panel-controls")
                                                        li()
                                                            a(class="panel-fullscreen", ng-click="estudio.edit = !estudio.edit")
                                                                span(class="fa fa-pencil", ng-hide="estudio.edit")
