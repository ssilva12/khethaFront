        //-div(class="row")
        div(class="col-md-6")
            div(class="panel panel-default")
                div(class="panel-heading")
                    h3(class="panel-title") Experiencias
                 div(class="panel-body")
                    div(class="form-group")
                        div(class="col-md-12")
                            button(type="button", class="btn btn-primary pull-right", ng-click="agregarExperiencia()") Agregar
                    div(class="form-group")
                        div(class="col-md-12")
                            div(class="dd", id="nestable")
