                    h3(class="panel-title") Idiomas
                div(class="panel-body")
                    div(class="form-group")
                        div(class="col-md-12")
                            button(type="button", class="btn btn-primary pull-right", ng-click="agregarIdioma()") Agregar
                    div(class="form-group")
                        div(class="col-md-12")
