//- PAGE TITLE
div(class="page-title")
    h2() Candidato:&nbsp;
        span(class="txt-grises", ng-bind="usuario.candidateInfo.name")
//- END PAGE TITLE
//- PAGE CONTENT WRAPPER
div(class="page-content-wrap")
    div(class="row")
        div(class="col-md-12")
            div(class="alert alert-danger", role="alert", style="display: none")
                button(type="button", class="close", data-dismiss="alert")
                    span(aria-hidden="true") x
                    span(class="sr-only") Close
                strong() Importante!
                span() &nbsp;Aqui se podrán visualizar todos los mensajes de alerta según las acciones que estemos efectuando en la pantalla.
    //- CANDIDATOS
    form(name="formCandidato", novalidate)
        div(class="row")
            div(class="col-md-3",style="display:none;")
                //- START PANEL WITH STATIC CONTROLS
                div(class="panel panel-default")
                    div(class="panel-heading")
                        div(class="form-group")
                            div(class="col-md-9 col-xs-9")
                                h3(class="profile-data-name", ng-bind="usuario.candidateInfo.name")
                                div(class="profile-data-title", ng-bind="usuario.candidateInfo.title")
                            div(class="col-md-3 col-xs-3")
                                ul(class="panel-controls")
                                    li()
                                        a(class="panel-edit", ng-click="usuario.edit = !usuario.edit")
                                            span(class="fa fa-pencil", ng-hide="usuario.edit")
                                            span(class="fa fa-save", ng-show="usuario.edit", ng-click="actualizarCandidato()")
                    div(class="panel-body")
                        div(class="text-center profilecand", id="user_image")
                            img(src="assets/images/users/user2.jpg", class="img-thumbnail")
                        br()
                        div(class="form-group")
                            div(class="col-md-4 col-xs-4")
                                label(for="") #ID
                            div(class="col-md-8 col-xs-8")
                                p(ng-bind="usuario.candidateInfo.id")
                        div(class="form-group")
                            div(class="col-md-4 col-xs-4")
                                label(for="") UserName
                            div(class="col-md-8 col-xs-8")
                                p(ng-bind="usuario.candidateInfo.username")
                        div(class="form-group")
                            div(div class="col-md-4 col-xs-4")
                                label(for="") Email
                            div(class="col-md-8 col-xs-8")
                                p()
                                    a(href="#", ng-bind="usuario.candidateInfo.email")
                //- END PANEL WITH STATIC CONTROLS
            div(class="col-md-9", ng-class="{'col-md-12': variablesGlobales.expandir,'col-md-9' : !variablesGlobales.expandir}")
                //- START PANEL WITH STATIC CONTROLS
                div(class="panel panel-default")
                    div(class="panel-heading")
                        h3(class="panel-title") Ficha del candidato
                        ul(class="panel-controls")
                            li()
                                a(class="panel-fullscreen", ng-click="variablesGlobales.expandir= !variablesGlobales.expandir")
                                    span(class="fa fa-expand")
                            li(class="dropdown", style="display:none;")
                                a(href="#", class="dropdown-toggle", data-toggle="dropdown")
                                    span(class="fa fa-cog")
                                ul(class="dropdown-menu")
                                    li()
                                        a(href="#", class="panel-collapse")
                                            span(span class="fa fa-angle-down")
                                            span() Collapse
                                    li()
                                        a(href="#", class="panel-refresh")
                                            span(class="fa fa-refresh")
                                            span() Refresh
                            li()
                                a(class="panel-remove", style="display:none;")
                                    span(class="fa fa-times")
                    div(class="panel-body")
                        div(class="form-group clearfix", ng-class="{ 'has-error' : formCandidato.nombre.$invalid && !formCandidato.nombre.$pristine }")   
                            label(class="col-md-3 control-label") Nombre Completo:
                            div(class="col-md-9")
                                input(name="nombre", type="text", class="form-control", ng-model="usuario.candidateInfo.name", required="") 
                                //-p(ng-show="formCandidato.nombre.$invalid", class="help-block") Nombre requerido. 
                        div(class="form-group clearfix")
                            label(class="col-md-3 control-label") Sexo:
                            //-select(style="height: 36px; padding: 0px 10px !important", class="form-control",data-ng-model="user.sexo", data-ng-options="sexo.value as sexo.label for sexo in variablesGlobales.sexo")
                            div(class="col-md-9").
                                    <select class="form-control" select2="", name="sexo" data-ng-model="usuario.candidateInfo.sex" >
                                        <option value="">Seleccione</option>
                                        <option ng-repeat="c in variablesGlobales.sexo" value="c.value">{{c.label}}</option>
                                    </select>
                        div(class="form-group clearfix")
                            label(class="col-md-3 control-label") Estatus:
                            div(class="col-md-9")
                                div(class="input-group")
                                    span(class="input-group-btn")
                                        button(class="btn btn-default", type="button")
                                            i(class="fa fa-circle status-disponible")
                                    select(class="form-control", select2="", name="estado", data-ng-model="usuario.candidateInfo.status")
                                        option(value="") Seleccione
                                        option(ng-repeat="estado in variablesGlobales.estados", value="estado.value") {{estado.label}}
                        div(class="form-group clearfix")
                            label(class="col-md-3 control-label") Número de identificación:
                            div(class="col-md-9")
                                input(type="text", class="form-control", placeholder="", ng-model="usuario.candidateInfo.idNumber")
                        div(class="form-group clearfix")
                            label(class="col-md-3 control-label") Teléfono:
                            div(class="col-md-9")
                                input(type="text", class="form-control", placeholder="", ng-model="usuario.candidateInfo.formattedNumber")
                        div(class="form-group clearfix", ng-class="{ 'has-error' : formCandidato.email.$invalid && !formCandidato.email.$pristine }")
                            label(class="col-md-3 control-label") E-mail:
                            div(class="col-md-9")
                                input(name="email", type="email", class="form-control", placeholder="", ng-model="usuario.candidateInfo.email")
                        div(class="form-group clearfix")
                            label(class="col-md-3 control-label") Dirección:
                            div(class="col-md-9")
                                input(type="text", class="form-control", placeholder="", ng-model="usuario.candidateInfo.addressLine")                    
                        div(class="form-group clearfix")
                            label(class="col-md-3 control-label") Población:
                            div(class="col-md-9")
                                select(class="form-control", select2="", name="ciudad", data-ng-model="usuario.candidateInfo.poblacion", ng-options="ciudad.value as ciudad.label for ciudad in variablesGlobales.poblacion")
                                        option(value="") Seleccione
                        div(class="form-group clearfix")
                            label(class="col-md-3 control-label") Código postal:
                            div(class="col-md-9")
                                input(type="text", class="form-control", placeholder="", ng-model="usuario.candidateInfo.codigoPostal")
                        div(class="form-group clearfix")
                            label(class="col-md-3 control-label") Pais:
                            div(class="col-md-9")
                                select(class="form-control", select2="", name="pais", data-ng-model="usuario.candidateInfo.pais", ng-options="pais.value as pais.label for pais in variablesGlobales.paises")
                                        option(value="") Seleccione
                        div(class="form-group clearfix")
                            label(class="col-md-3 control-label") Nacionalidad:
                            div(class="col-md-9")
                                select(class="form-control", select2="", name="nacionalidad", data-ng-model="usuario.candidateInfo.nacionalidad", ng-options="nacionalidad.value as nacionalidad.label for nacionalidad in variablesGlobales.paises")
                                        option(value="") Seleccione
                        div(class="form-group")
                            div(class="col-md-12")
                                button(type="button", class="btn btn-primary pull-right", ng-click="actualizarCandidato()") 
                                    li(class="fa fa-save")
                                    span() Guardar
                //- END PANEL WITH STATIC CONTROLS
            div(class="col-md-3")
                //- START PANEL WITH STATIC CONTROLS
                div(class="panel panel-default")
                    div(class="panel-heading")
                        h3(class="panel-title") Información Adicional
                    div(class="panel-body")
                        h4(class="txt-redAlert") Esta información no se puede editar
                        div(class="form-group")
                            div(class="col-md-12")
                                label(for="") Coordenadas GPS
                            div(class="col-md-12")
                                p.
                                    &nbsp; &nbsp;<strong>Long:</strong> {{usuario.candidateInfo.long}}
                                p.
                                    &nbsp; &nbsp;<strong>Lat:</strong> {{usuario.candidateInfo.lat}}
                        div(class="form-group")
                            div(class="col-md-4 col-xs-4")
                                label(for="") Desde
                            div(class="col-md-8 col-xs-8")
                                p(ng-bind="usuario.candidateInfo.desde")
                        div(class="form-group")
                            div(class="col-md-4 col-xs-4")
                                label(for="") Última Actualización
                            div(class="col-md-8 col-xs-8")
                                p(ng-bind="usuario.candidateInfo.ultimaAct")
                //- END PANEL WITH STATIC CONTROLS
                div(class="panel panel-default", style="display:none;")
                    div(class="panel-heading")
                        h3(class="panel-title") Categorias del candidato
                    div(class="panel-body")
                        ul(class="list-tags")
                            li(ng-repeat="categoria in user.categorias")
                                a(href="#")
                                    span(class="fa fa-tag")
                                    span(ng-bind="categoria")
    //- FIN CANDIDATOS
    div(class="row")
        div(class="col-md-6")
            div(class="panel panel-default")
                div(class="panel-heading")
                    h3(class="panel-title") Estudios del candidato
                div(class="panel-body")
